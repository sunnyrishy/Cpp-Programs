#include<iostream>
using namespace std;

int main() {
    int sum =0;
    int arr[5];
    cout<<"Enter 5 array elements : ";
    int n;
    for(int i=0;i<sizeof(arr)/sizeof(arr[0]);i++) {
        cin>>arr[i];
    }
    for(int i=0;i<sizeof(arr)/sizeof(arr[0]);i++) {
        sum += arr[i];
    }
    cout<<sum;
}
