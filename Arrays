Creation of arrays and printing the values of array.

#include<iostream>
using namespace std;

int main() {
    int arr[5];
    cout<<"Enter 5 Elements : "<<endl;
    
    for(int i=0;i<5;i++) {
        cin>>arr[i];
    }
    
    for(int j=0;j<sizeof(arr)/sizeof(arr[0]);j++) {
        cout<<arr[j]<<endl;
    }
    
    
}
